<AnchorPane id="AnchorPane" prefHeight="326.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BookingCompleteFXML">
   <children>
      <ImageView fitHeight="326.0" fitWidth="380.0" pickOnBounds="true">
         <image>
            <Image url="@whiteimage.jpg" />
         </image>
      </ImageView>
      <Text fill="#0095eb" layoutX="81.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Thank you for your patience!">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Text>
      <Text layoutX="12.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please enter your Card No. to buy Ticket">
