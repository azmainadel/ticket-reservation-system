            <Font size="20.0" />
         </font>
      </Text>
      <Label fx:id="CardStatus" layoutX="94.0" layoutY="188.0" prefHeight="41.0" prefWidth="196.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="Finish" layoutX="129.0" layoutY="266.0" mnemonicParsing="false" onAction="#FinishButton" prefHeight="30.0" prefWidth="126.0" text="Finish">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <PasswordField fx:id="CardInput" alignment="CENTER" layoutX="43.0" layoutY="129.0" prefHeight="53.0" prefWidth="189.0" promptText="Card No." />
      <Button fx:id="Confirm" layoutX="241.0" layoutY="140.0" mnemonicParsing="false" onAction="#ConfirmButton" prefHeight="30.0" prefWidth="126.0" text="Confirm">
